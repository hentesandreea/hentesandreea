const myAge = 20;
let earlyYears = 2;
earlyYears = 10.5 * earlyYears;
let laterYears = myAge - 2;
laterYears = 4 * laterYears;
console.log(earlyYears);
console.log(laterYears);
let myAgeInDogYears = earlyYears + laterYears;
let myName = 'Andreea';
myName = 'Andreea'.toUpperCase();
console.log(`My name is ${myName}. I am ${myAge} old in human years which is ${myAgeInDogYears} old in dog years.`);
